{
    "plugins": [
        "comment",
        "footnote-string-to-number",
        "hints",
        "youtube",
        "image-captions",
        "simple-page-toc"
    ],
    "pluginsConfig": {
        "comment": {
            "highlightCommented": true
        },
        "image-captions": {
          "caption": "Imagen _PAGE_IMAGE_NUMBER_ - _CAPTION_"
        },
        "page-toc": {
            "selector": ".markdown-section h1, .markdown-section h2, .markdown-section h3, .markdown-section h4",
            "position": "before-first"
        },
        "simple-page-toc": {
            "maxDepth": 3,
            "skipFirstH1": true
        }
    }
}
